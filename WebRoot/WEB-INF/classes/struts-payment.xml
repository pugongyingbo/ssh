<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="payment" extends="struts-default" namespace="">
	
		<action name="customerremain" class="paymentaction" method="getRemain">
			<result name="success">person/customerjiaofei.jsp</result>
		</action>


		<action name="recharge" class="paymentaction" method="recharge">
			<result name="success">person/logBySidSucc.jsp</result>
		</action>


		<action name="customerpayrecord" class="paymentaction" method="getPayRecord">
			<result name="success">person/payed.jsp</result>
		</action>
	</package>


</struts>